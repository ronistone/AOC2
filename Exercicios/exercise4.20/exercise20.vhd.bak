entity priority_encoder is
port(a:
in STD_LOGIC_VECTOR(7 downto 0);
y:
out STD_LOGIC_VECTOR(2 downto 0);
none: out STD_LOGIC);
end;
architecture synth of
begin
process(all) begin
case? a is
when "00000000"
when "00000001"
when "0000001-"
when "000001--"
when "00001---"
when "0001----"
when "001-----"
when "01------"
when "1-------"
when others
end case?;
end process;
end;